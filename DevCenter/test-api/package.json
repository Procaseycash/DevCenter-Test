{
  "name": "test",
  "version": "2.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "mocha src/**/*.spec.ts --require ts-node/register --reporter spec",
    "start": "node index.js",
    "watch": "tsc -w",
    "serve": "nodemon -e ts html --watch src --exec \"npm run build && npm run html && npm run css && npm run js && npm run img && npm run start\" ",
    "html": "copyfiles -f src/app/views/*.html bin/app/views/",
    "css": "copyfiles -f src/app/public/**/*.css bin/app/public/css/",
    "img": "copyfiles -f src/app/public/**/*.jpg src/app/public/**/*.png src/app/public/**/*.jpg src/app/public/**/*.gif  bin/app/public/img/",
    "js": "copyfiles -f src/app/public/**/*.js bin/app/public/js/"
  },
  "dependencies": {
    "@nestjs/common": "^4.0.2",
    "@nestjs/core": "^4.0.2",
    "@nestjs/microservices": "^4.0.2",
    "@nestjs/testing": "^4.0.2",
    "@nestjs/websockets": "^4.0.2",
    "axios": "^0.18.0",
    "bootstrap": "^4.0.0",
    "consolidate": "^0.15.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.12.9",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.3",
    "swig": "^1.4.2"
  },
  "devDependencies": {
    "@types/chai": "^4.0.2",
    "@types/express": "^4.0.36",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.19",
    "@types/sinon": "^2.3.3",
    "chai": "^4.1.1",
    "copyfiles": "^1.2.0",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "sinon": "^2.4.1",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.2"
  }
}
